
@{
    ViewBag.Title = "Admin Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .container {
        max-width: 1200px;
        margin: 0 auto;
    }

    .jumbotron {
        background: none;
        color: #333;
        text-align: center;
        padding: 40px;
        margin-bottom: 20px;
        margin-top: 50px;
    }

    h1 {
        font-size: 36px;
        margin-bottom: 20px;
    }

    .row {
        display: flex;
        flex-wrap: wrap;
        justify-content: space-between;
        gap: 20px;
        margin-top: 30px;
    }

    .col-md-4 {
        flex-basis: calc(33.33% - 20px);
    }

    .card {
        width: 100%;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: space-between;
        border: 1px solid #ddd;
        padding: 20px;
        background-color: #fff;
        box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);
    }

        .card img {
            max-width: 100%;
            max-height: 100px;
            object-fit: contain;
        }

    .card-title {
        font-size: 18px;
        font-weight: bold;
        margin-top: 10px;
    }

    .card-text {
        font-size: 14px;
        color: #555;
        margin-top: 10px;
    }

    .btn-card {
        margin-top: 15px;
        width: 100%;
    }
</style>

<div class="container mt-5">
    <div class="jumbotron">
        <h1 class="display-4">Welcome to Admin Dashboard</h1>
        <p class="lead">Manage your admin tasks efficiently.</p>
    </div>

